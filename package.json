{
  "name": "ton-blueprint",
  "displayName": "Ton Blueprint",
  "description": "Developing Ton project with Blueprint",
  "publisher": "MahdiKarami",
  "version": "1.0.0",
  "icon": "media/blueprint.png",
  "engines": {
    "vscode": "^1.83.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MKdir98/blueprint-vscode-extension"
  },
  "extensionKind": [
    "workspace"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "blueprint-sidebar-view",
          "title": "Blueprint",
          "icon": "media/blueprint.svg"
        }
      ]
    },
    "views": {
      "blueprint-sidebar-view": [
        {
          "type": "webview",
          "id": "blueprint-sidebar",
          "name": "Contracts",
          "contextualTitle": "Contracts"
        }
      ]
    }
  },
  "scripts": {
    "test-compile": "yarn clean-out && -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "ci": "yarn lint && yarn test --coverage",
    "clean-out": "rimraf ./out",
    "vscode:prepublish": "yarn install --only=prod && yarn clean-out && yarn compile",
    "compile": "rollup -c && tsc",
    "watch": "concurrently \"rollup -c -w\" \"tsc \"",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" \"*.json\"  \"*.js\" ",
    "test": "jest",
    "watch-test": "yarn test -- --watch",
    "tsc": "tsc --noEmit"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^11.1.5",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/polka": "^0.5.1",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.31.0",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.3",
    "ts-loader": "^8.0.11",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@ton/blueprint": "0.14.2",
    "@ton/core": "^0.53.0",
    "@ton/crypto": "^3.2.0",
    "@ton/sandbox": "^0.13.1",
    "@ton/test-utils": "^0.4.2",
    "@ton/ton": "^13.9.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.2.5",
    "arg": "^5.0.2",
    "fs.promises": "^0.1.2",
    "iconify": "^1.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
